<h1 align=center><i>User List</i></h1>

<table align=center border=1 cellspacing=0>
  <tr>
    <th>id</th>
    <th>Name</th>
    <th>Language</th>
  </tr>

<% @ms_users.each do |ms_user| %>
  <tr>
    <td><%= ms_user.id %></td>
    <td><%= ms_user.name %></td>
    <td><%= ms_user.language %></td>
  </tr>
<% end %>
</table>

<br />

<p align=center><i><%= link_to 'Register New Member', new_ms_user_path %></i></p>
